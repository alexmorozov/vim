snippet class
	/**
	 * ${1}
	 */
	class ${2:ClassName}
	{
		${3}
		function ${4:__construct}($${5:argument})
		{
			${6:// code...}
		}
	}
snippet func
	/**
	 * ${1}
	 */
	function ${2:FunctionName}($${3:argument}) {
		${4:// code...}
		return $${5:return};
	}
snippet cfunction
	${1:public }function ${2:FunctionName}(${3})
	{
		${4:// code...}
	}
